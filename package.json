{
  "name": "@mighty-justice/smart-bool",
  "version": "2.0.0",
  "description": "Eliminate your single-line boolean setting methods",
  "main": "dist/index.js",
  "module": "dist/SmartBool.esm.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "analyze": "source-map-explorer 'dist/*.js'",
    "build": "tsdx build",
    "deploy": "np",
    "dev": "rollup -c -w",
    "format": "tsdx lint --fix",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "preversion": "npm run lint",
    "start": "tsdx watch",
    "test": "tsdx test",
    "version": "npm run build && git add dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mighty-justice/smart-bool.git"
  },
  "browserslist": [
    "> 5%"
  ],
  "keywords": [
    "javascript",
    "typescript",
    "mobx",
    "utils"
  ],
  "author": "Mighty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mighty-justice/smart-bool/issues"
  },
  "homepage": "https://mighty-justice.github.io/smart-bool/",
  "prettier": {
    "printWidth": 120,
    "proseWrap": "always",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "src/**/*.ts"
    ],
    "setupFilesAfterEnv": [
      "./test/setup.ts"
    ]
  },
  "files": [
    "dist",
    "index.js"
  ],
  "peerDependencies": {
    "mobx": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "coveralls": "^3",
    "mobx": "^5",
    "np": "^4",
    "source-map-explorer": "^2.5.0",
    "tsdx": "^0",
    "typescript": "^3"
  },
  "dependencies": {
    "class-autobind-decorator": "^3"
  }
}
