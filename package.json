{
  "name": "@mighty-justice/smart-bool",
  "version": "0.3.2",
  "description": "Eliminate your single-line boolean setting methods",
  "main": "dist/SmartBool.cjs.js",
  "module": "dist/SmartBool.esm.js",
  "browser": "dist/SmartBool.umd.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:types && npm run build:js",
    "build:clean": "rm -r dist/* || true",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "deploy": "np",
    "dev": "rollup -c -w",
    "docs:build": "npm run docs:prepare && gitbook build",
    "docs:deploy": "gh-pages -d _book",
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "lint": "tslint --project .",
    "postpublish": "npm run docs:build && npm run docs:deploy",
    "preversion": "npm run lint",
    "test": "jest",
    "test:watch": "npm run test/*.js -- -w",
    "version": "npm run build && git add dist",
    "view-source-map": "source-map-explorer --html dist/index.js dist/index.map > source.html && open source.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mighty-justice/smart-bool.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "mobx",
    "utils"
  ],
  "author": "Mighty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mighty-justice/smart-bool/issues"
  },
  "homepage": "https://mighty-justice.github.io/smart-bool/",
  "jest": {
    "testPathIgnorePatterns": [
      "/_book/",
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "setupFilesAfterEnv": ["./test/setup.js"],
    "testURL": "http://localhost/"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "peerDependencies": {
    "mobx": "^4.0.0 || ^5.0.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@mighty-justice/tslint-config": "^0.3.0",
    "babelrc-rollup": "^3.0.0",
    "coveralls": "^3.0.3",
    "gh-pages": "^2.0.1",
    "gitbook-cli": "^2.3.2",
    "gitbook-plugin-anchorjs": "^2.1.0",
    "gitbook-plugin-search-plus": "^1.0.4-alpha-3",
    "jest": "^24.7.1",
    "jest-environment-jsdom": "^24.7.1",
    "mobx": "^5.9.4",
    "np": "^4.0.2",
    "regenerator-runtime": "^0.13.2",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.1",
    "source-map-explorer": "^1.8.0",
    "tslint": "^5.15.0",
    "typescript": "^3.4.2"
  },
  "dependencies": {
    "class-autobind-decorator": "^3.0.1"
  }
}
