{
  "name": "@mighty-justice/smart-bool",
  "version": "0.2.0",
  "description": "Eliminate your single-line boolean setting methods",
  "main": "dist/SmartBool.cjs.js",
  "module": "dist/SmartBool.esm.js",
  "browser": "dist/SmartBool.umd.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -r dist/*",
    "build:types": "yarn tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build": "npm run build:types && npm run build:js",
    "dev": "rollup -c -w",
    "lint": "tslint src/*.tsx?",
    "test": "jest",
    "test:watch": "npm run test/*.js -- -w",
    "prebuild": "npm run lint",
    "predeploy": "npm run build && npm run docs:build",
    "deploy": "gh-pages -d _book",
    "docs:prepare": "gitbook install",
    "docs:build": "npm run docs:prepare && gitbook build",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "view-source-map": "source-map-explorer --html dist/index.js dist/index.map > source.html && open source.html",
    "release": "np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mighty-justice/smart-bool.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "mobx",
    "utils"
  ],
  "author": "Robert Kuykendall <robert@mighty.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mighty-justice/smart-bool/issues"
  },
  "homepage": "https://mighty-justice.github.io/smart-bool/",
  "jest": {
    "testPathIgnorePatterns": [
      "/_book/",
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "testURL": "http://localhost/"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "peerDependencies": {
    "mobx": ">=5.8"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/polyfill": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "babelrc-rollup": "^3.0.0",
    "class-autobind-decorator": "^3.0.1",
    "coveralls": "^3.0.2",
    "gh-pages": "^2.0.1",
    "gitbook-cli": "^2.3.2",
    "gitbook-plugin-anchorjs": "^2.1.0",
    "gitbook-plugin-search-plus": "^1.0.4-alpha-3",
    "jest": "^23.6.0",
    "jest-environment-jsdom": "^23.4.0",
    "mobx": "^5.8.0",
    "np": "^3.1.0",
    "regenerator-runtime": "^0.13.1",
    "rollup": "^0.68.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "source-map-explorer": "^1.6.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
  }
}
