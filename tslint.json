{
  "extends": ["tslint:recommended", "tslint-react", "tslint-eslint-rules"],
  "rules": {
    "arrow-parens": false,
    "arrow-return-shorthand": true,
    "cyclomatic-complexity": [true, 8],
    "deprecation": true,
    "eofline": true,
    "indent": [true, "spaces", 2],
    "jsx-boolean-value": false,
    "jsx-curly-spacing": "never",
    "jsx-equals-spacing": "never",
    "jsx-key": true,
    "jsx-no-bind": true,
    "jsx-no-lambda": true,
    "jsx-no-multiline-js": false,
    "jsx-no-string-ref": true,
    "jsx-self-close": true,
    "jsx-wrap-multiline": true,
    "match-default-export-name": false,
    "max-classes-per-file": [true, 1],
    "max-line-length": false,
    "member-ordering": false,
    "no-consecutive-blank-lines": true,
    "no-console": true,
    "no-debugger": true,
    "no-duplicate-imports": true,
    "no-duplicate-super": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-empty-interface": true,
    "no-invalid-template-strings": true,
    "no-invalid-this": true,
    "no-magic-numbers": [true, -1, 0, 1, 2, 3, 4, 6, 8, 10, 12, 16, 60, 100, 1000],
    "no-misused-new": true,
    "no-multi-spaces": true,
    "no-require-imports": true,
    "no-string-literal": false,
    "no-string-throw": true,
    "no-this-assignment": true,
    "no-trailing-whitespace": true,
    "no-unnecessary-class": true,
    "no-unsafe-finally": true,
    "no-unused-expression": true,
    "no-unused-variable": true,
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "object-literal-sort-keys": [true, "ignore-case"],
    "one-line": false,
    "one-variable-per-declaration": false,
    "ordered-imports": false,
    "prefer-const": true,
    "prefer-object-spread": true,
    "promise-function-async": true,
    "quotemark": [true, "single", "jsx-single"],
    "restrict-plus-operands": true,
    "semicolon": [true, "always"],
    "space-before-function-paren": [true, "always"],
    "switch-default": true,
    "variable-name": [true, "check-format", "allow-pascal-case", "allow-leading-underscore"]
  }
}
